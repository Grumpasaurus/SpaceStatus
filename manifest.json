{

  "manifest_version": 2,

  "name": "SpaceStatus Alpha",
  "description": "Need to check quickly if your favorite HackerSpace hash open?",
  "version": "1.0",

  "browser_action": {
    "default_icon": {
      "19": "Images/logo.png",
      "38": "Images/logo.png"
    },
    "default_popup": "popup.html"
  },

   "permissions": [
    "cookies",
    "notifications",
    "https://raw.github.com/SpaceApi/OpenSpaceDirectory/master/directory.json"
  ],

  "web_accessible_resources": [
    "Images/logo.png"
  ],

  "background": {
    "scripts": [
      "JavaScript/jquery-1.9.1.js",
      "JavaScript/popup.js"
    ]
  }

}